{
    "name": "web-id/cafe",
    "description": "This is my package Cafe",
    "keywords": [
        "web-id",
        "laravel",
        "cafe"
    ],
    "homepage": "https://github.com/web-id/cafe",
    "license": "MIT",
    "authors": [
        {
            "name": "LÃ©o TIOLLIER",
            "email": "ltiollier30@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "WebId\\Cafe\\": "src",
            "WebId\\Cafe\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebId\\Cafe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebId\\Cafe\\CafeServiceProvider"
            ],
            "aliases": {
                "Cafe": "WebId\\Cafe\\CafeFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
